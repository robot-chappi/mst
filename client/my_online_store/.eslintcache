    fetchBasketDevices(idd.id).then(data => setDevice(data)).finally(() => setLoading(false))\r\n    }, [])\r\n\r\n    const deleteItem = () => {\r\n        let idd = jwt_decode(localStorage.getItem('token'))\r\n        deleteBasketDevices(idd.id)\r\n        message.success('The basket has been emptied. \\nRefresh the page...')\r\n    }\r\n\r\n    const deleteOneItem = (product) => {\r\n        let idd = jwt_decode(localStorage.getItem('token'))\r\n        console.log(idd.id)\r\n        console.log(product)\r\n        deleteOneDeviceBasket(idd.id, product)\r\n        message.success('The product has been removed. \\nRefresh the page...')\r\n    }\r\n\r\n    const newsletterSub = async () => {\r\n        const formData = new FormData()\r\n        formData.append('email', newsletter)\r\n        createNewsletterSub(formData)\r\n        message.success('You have subscribed to the newsletter!')\r\n    }\r\n\r\n    const IconFont = createFromIconfontCN({\r\n        scriptUrl: '//at.alicdn.com/t/font_8d5l8fzk5b87iudi.js',\r\n      });\r\n\r\n    if (loading) {\r\n        return <Spinner animation={'grow'}/>\r\n      }\r\n\r\n    \r\n\r\n    return (\r\n        <Layout>\r\n            {isDesktopOrLaptop &&\r\n            <Content>\r\n                <Row style={{paddingTop: 20, paddingBottom: 30}}>\r\n                    <Col span={20} offset={4}>\r\n                        <Col span={20}>\r\n                            <h4 style={{fontWeight: 600, fontFamily: 'Poppins', fontSize: 35}}>Shopping Cart</h4>\r\n                        </Col>\r\n                        <Row className=\"d-flex\">\r\n                        {device.map(num => ( \r\n                            <Col md={3} className='mt-3'>\r\n                            <Card onClick={() => history.push(DEVICE_ROUTE + '/' + num.id)} style={{width: 185, color: 'black', cursor: 'pointer', fontFamily: 'Poppins', fontSize: 16, fontWeight: 600, borderRadius: 15, paddingTop: 5, background: 'white'}}>\r\n                 